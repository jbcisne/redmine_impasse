<tr id="<%= dom_id impasse_exec_step_hist %>" class="<%= cycle "odd", "even" %>">
  <td><%= impasse_exec_step_hist.test_steps %></td>
  <td><%= impasse_exec_step_hist.test_plan_case %></td>
  <td><%= impasse_exec_step_hist.issue %></td>
  <td><%= authoring impasse_exec_step_hist.created_at, impasse_exec_step_hist.author %>.</td>
  <td><%= l(:label_updated_time, time_tag(impasse_exec_step_hist.updated_at)).html_safe %>.</td>
  <td class="buttons">
    <%= link_to l(:button_show), impasse_exec_step_hist, remote: true %>
    <%= link_to l(:button_edit), edit_impasse_exec_step_hist_path(impasse_exec_step_hist), class: "icon icon-edit", remote: true %>
    <%= link_to l(:button_delete), impasse_exec_step_hist, method: :delete, data: { confirm: l(:text_are_you_sure) }, class: "icon icon-del", remote: true %>
  </td>
</tr>