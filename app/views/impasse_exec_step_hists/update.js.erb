<% if @impasse_exec_step_hist.valid? %>
  hideModal();
  if ($(".impasse_exec_step_hists.list").length) {
    $("#<%= dom_id @impasse_exec_step_hist %>").replaceWith("<%= j render(@impasse_exec_step_hist) %>");
    $("#<%= dom_id @impasse_exec_step_hist %>").effect("highlight");
  } else {
    window.location.href = "<%= url_for @impasse_exec_step_hist %>";
  }
<% else %>
  $("#errorExplanation").remove();
  $("<%= j error_messages_for("impasse_exec_step_hist") %>").prependTo("#edit_impasse_exec_step_hist_<%= @impasse_exec_step_hist.id %>");
<% end %>