<% if @impasse_exec_step_hist.valid? %>
  hideModal();
  if ($(".impasse_exec_step_hists.list").length) {
    $(".nodata").hide();
    $(".impasse_exec_step_hists").show();
    $("<%= j render(@impasse_exec_step_hist, formats: [:html]) %>").prependTo(".impasse_exec_step_hists tbody").effect("highlight");
  } else {
    window.location.href = "<%= url_for @impasse_exec_step_hist %>";
  }
<% else %>
  $("#errorExplanation").remove();
  $("<%= j error_messages_for("impasse_exec_step_hist") %>").prependTo("#new_impasse_exec_step_hist");
<% end %>